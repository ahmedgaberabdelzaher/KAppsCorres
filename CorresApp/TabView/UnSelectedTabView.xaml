<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="CorresApp.TabView.UnSelectedTabView">
    <ContentView.Content>
                   <StackLayout x:Name="StackLayout">
                         <Image x:Name="image" HeightRequest="30"></Image>
                         <Label x:Name="Text" TextColor="{StaticResource placHolderColor}" HorizontalOptions="CenterAndExpand"></Label>
</StackLayout>
    </ContentView.Content>
</ContentView>
